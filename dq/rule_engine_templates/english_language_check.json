{
    "database": "$database",
    "table": "$table",
    "columns": "$columns",
    "dimension": "validity",
    "sub_category": "English Character Check",
    "severity": "",
    "composite_columns": "$composite_columns",
    "critical_attribute": "$critical_attribute",
    "model_integrity": "$model_integrity",
    "filter_conditions": [
        {
            "field": "$columns",
            "operation": "REGEX",
            "pattern": "^[a-zA-Z]+([-.\\/ ][a-zA-Z]+)*\\s*$",
            "exclude_flag": 1
        }
    ]
}